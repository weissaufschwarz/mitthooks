{
    "name": "@weissaufschwarz/mitthooks",
    "version": "0.2.1",
    "private": false,
    "type": "module",
    "exports": {
        "./*.js": "./dist/*.js",
        "./*": "./dist/*.js"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com/weissaufschwarz/mitthooks.git",
        "directory": "packages/mitthooks"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "test": "vitest run",
        "lint": "eslint 'src/**/*.ts' --config .eslintrc.cjs",
        "format": "prettier --write -c ./.prettierrc '**/*.{ts,yaml,yml,json,md}'"
    },
    "dependencies": {
        "@mittwald/api-client": "^4.61.1",
        "@noble/ed25519": "^2.1.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@repo/eslint-config": "workspace:*",
        "@types/node": "^22.7.3",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "@vercel/style-guide": "^5.2.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-turbo": "^2.0.0",
        "eslint-plugin-only-warn": "^1.1.0",
        "prettier": "^3.3.3",
        "turbo": "^2.1.3",
        "typescript": "^5.6.2",
        "vitest": "^3.1.1"
    }
}
